#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int N,sum;
    cin>>N;
    for (int i=0;i<N;i++){
        int j;
        cin>>j;
        sum+=j;
    }
    cout<<sum<<endl;
    return 0;
}
