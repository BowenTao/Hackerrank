#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

string func(int h){
    switch(h){
        case 1: return "one";
        case 2: return "two";
        case 3: return "three";
        case 4: return "four";
        case 5: return "five";
        case 6: return "six";
        case 7: return "seven";
        case 8: return "eight";
        case 9: return "nine";
        case 10: return "ten";
        case 11: return "eleven";
        case 12:return "twelve";
        case 13: return "one";
        default : return "default_hour";
    }
}

int main(){
    int h;
    cin >> h;
    int m;
    cin >> m;
    switch(m){
        case 0:cout<<func(h)<<" o' clock"<<endl; return 0;
            break;
        case 1:cout<<"one minute past "<<func(h)<<endl; return 0; break;
        case 2:cout<<"two minutes past "<<func(h)<<endl; return 0; break;
        case 3:cout<<"three minutes past "<<func(h)<<endl; return 0; break;
        case 4:cout<<"four minutes past "<<func(h)<<endl; return 0; break;
        case 5:cout<<"five minutes past "<<func(h)<<endl; return 0; break;
        case 6:cout<<"six minutes past "<<func(h)<<endl; return 0; break;
        case 7:cout<<"seven minutes past "<<func(h)<<endl; return 0; break;
        case 8:cout<<"eight minutes past "<<func(h)<<endl; return 0; break;
        case 9:cout<<"nine minutes past "<<func(h)<<endl; return 0; break;
        case 10:cout<<"ten minutes past "<<func(h)<<endl; return 0; break;
        case 11:cout<<"eleven minutes past "<<func(h)<<endl; return 0; break;
        case 12:cout<<"twelve minutes past "<<func(h)<<endl; return 0; break;
        case 13:cout<<"thirteen minutes past "<<func(h)<<endl; return 0; break;
        case 14:cout<<"fourteen minutes past "<<func(h)<<endl; return 0; break;
        case 15:cout<<"quarter past "<<func(h)<<endl; return 0; break;
        case 16:cout<<"sixteen minutes past "<<func(h)<<endl; return 0; break;
        case 17:cout<<"seventeen minutes past "<<func(h)<<endl; return 0; break;
        case 18:cout<<"eighteen minutes past "<<func(h)<<endl; return 0; break;
        case 19:cout<<"nineteen minutes past "<<func(h)<<endl; return 0; break;
        case 20:cout<<"twenty minutes past "<<func(h)<<endl; return 0; break;
        case 21:cout<<"twenty one minutes past "<<func(h)<<endl; return 0; break;
        case 22:cout<<"twenty two minutes past "<<func(h)<<endl; return 0; break;
        case 23:cout<<"twenty three minutes past "<<func(h)<<endl; return 0; break;
        case 24:cout<<"twenty four minutes past "<<func(h)<<endl; return 0; break;
        case 25:cout<<"twenty five minutes past "<<func(h)<<endl; return 0; break;
        case 26:cout<<"twenty six minutes past "<<func(h)<<endl; return 0; break;
        case 27:cout<<"twenty seven minutes past "<<func(h)<<endl; return 0; break;
        case 28:cout<<"twenty eight minutes past "<<func(h)<<endl; return 0; break;
        case 29:cout<<"twenty nine minutes past "<<func(h)<<endl; return 0; break;
        case 30:cout<<"half past "<<func(h)<<endl; return 0; break;
        
        case 59:cout<<"one minute to "<<func(h+1)<<endl; return 0; break;
        case 58:cout<<"two minutes to "<<func(h+1)<<endl; return 0; break;
        case 57:cout<<"three minutes to "<<func(h+1)<<endl; return 0; break;
        case 56:cout<<"four minutes to "<<func(h+1)<<endl; return 0; break;
        case 55:cout<<"five minutes to "<<func(h+1)<<endl; return 0; break;
        case 54:cout<<"six minutes to "<<func(h+1)<<endl; return 0; break;
        case 53:cout<<"seven minutes to "<<func(h+1)<<endl; return 0; break;
        case 52:cout<<"eight minutes to "<<func(h+1)<<endl; return 0; break;
        case 51:cout<<"nine minutes to "<<func(h+1)<<endl; return 0; break;
        case 50:cout<<"ten minutes to "<<func(h+1)<<endl; return 0; break;
        case 49:cout<<"eleven minutes to "<<func(h+1)<<endl; return 0; break;
        case 48:cout<<"twelve minutes to "<<func(h+1)<<endl; return 0; break;
        case 47:cout<<"thirteen minutes to "<<func(h+1)<<endl; return 0; break;
        case 46:cout<<"fourteen minutes to "<<func(h+1)<<endl; return 0; break;
        case 45:cout<<"quarter to "<<func(h+1)<<endl; return 0; break;
        case 44:cout<<"sixteen minutes to "<<func(h+1)<<endl; return 0; break;
        case 43:cout<<"seventeen minutes to "<<func(h+1)<<endl; return 0; break;
        case 42:cout<<"eighteen minutes to "<<func(h+1)<<endl; return 0; break;
        case 41:cout<<"nineteen minutes to "<<func(h+1)<<endl; return 0; break;
        case 40:cout<<"twenty minutes to "<<func(h+1)<<endl; return 0; break;
        case 39:cout<<"twenty one minutes to "<<func(h+1)<<endl; return 0; break;
        case 38:cout<<"twenty two minutes to "<<func(h+1)<<endl; return 0; break;
        case 37:cout<<"twenty three minutes to "<<func(h+1)<<endl; return 0; break;
        case 36:cout<<"twenty four minutes to "<<func(h+1)<<endl; return 0; break;
        case 35:cout<<"twenty five minutes to "<<func(h+1)<<endl; return 0; break;
        case 34:cout<<"twenty six minutes to "<<func(h+1)<<endl; return 0; break;
        case 33:cout<<"twenty seven minutes to "<<func(h+1)<<endl; return 0; break;
        case 32:cout<<"twenty eight minutes to "<<func(h+1)<<endl; return 0; break;
        case 31:cout<<"twenty nine minutes to "<<func(h+1)<<endl; return 0; break;
        
        case 60:cout<<func(h+1)<<" o' clock"<<endl; return 0;
            break;
    }
    return 0;
}
