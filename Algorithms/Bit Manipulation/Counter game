#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int t;
    cin>>t;
    while(t>0){
        unsigned long long n;
        cin>>n;
        int count=_popcnt64(n-1);
        //cout<<count<<endl;
        if (count&1){
            cout<<"Louise"<<endl;
        }else{
            cout<<"Richard"<<endl;
        }
        t--;
    }
    return 0;
}
