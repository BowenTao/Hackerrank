#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int t;

long long int n,i;
long long int m;
scanf("%d",&t);

while(t--){ 
   scanf("%lld",&n);
   i=2; 
   while(i<=(int)sqrt(n)){         
      while(n%i==0){              
         m=i;
         n/=i;                                         
       }
       i++;         
    }
    if (n>m){
        m=n;
    }
    printf("%lld\n",m);
}

    return 0;
}
