#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int t;

long long int n,i;

scanf("%d",&t);

while(t--){ 
   scanf("%lld",&n); 
   i=2;
   for(;i<=(int)sqrt(n);){         
      if(n%i==0){              
         n/=i;
         i=2;                                         
       }else{
          i++;
      }         
    }
    printf("%lld\n",n);
}

    return 0;
}
